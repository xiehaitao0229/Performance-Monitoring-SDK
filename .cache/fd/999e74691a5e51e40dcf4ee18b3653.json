}},{"source":"../src/performance/firstInput.ts","name":null,"original":{"line":68,"column":33},"generated":{"line":61,"column":52}},{"source":"../src/performance/firstInput.ts","name":null,"original":{"line":68,"column":34},"generated":{"line":61,"column":53}},{"source":"../src/performance/firstInput.ts","name":null,"original":{"line":70,"column":4},"generated":{"line":62,"column":4}},{"source":"../src/performance/firstInput.ts","name":null,"original":{"line":71,"column":4},"generated":{"line":63,"column":4}},{"source":"../src/performance/firstInput.ts","name":null,"original":{"line":72,"column":4},"generated":{"line":64,"column":4}},{"source":"../src/performance/firstInput.ts","name":null,"original":{"line":72,"column":4},"generated":{"line":64,"column":8}},{"source":"../src/performance/firstInput.ts","name":null,"original":{"line":72,"column":11},"generated":{"line":64,"column":20}},{"source":"../src/performance/firstInput.ts","name":null,"original":{"line":72,"column":12},"generated":{"line":64,"column":22}},{"source":"../src/performance/firstInput.ts","name":null,"original":{"line":72,"column":29},"generated":{"line":64,"column":39}},{"source":"../src/performance/firstInput.ts","name":null,"original":{"line":72,"column":31},"generated":{"line":64,"column":41}},{"source":"../src/performance/firstInput.ts","name":null,"original":{"line":72,"column":33},"generated":{"line":64,"column":52}},{"source":"../src/performance/firstInput.ts","name":null,"original":{"line":72,"column":34},"generated":{"line":64,"column":53}},{"source":"../src/performance/firstInput.ts","name":null,"original":{"line":72,"column":39},"generated":{"line":64,"column":58}},{"source":"../src/performance/firstInput.ts","name":null,"original":{"line":72,"column":40},"generated":{"line":64,"column":59}},{"source":"../src/performance/firstInput.ts","name":null,"original":{"line":73,"column":2},"generated":{"line":65,"column":2}},{"source":"../src/performance/firstInput.ts","name":null,"original":{"line":73,"column":3},"generated":{"line":65,"column":3}},{"source":"../src/performance/firstInput.ts","name":null,"original":{"line":73,"column":5},"generated":{"line":65,"column":5}},{"source":"../src/performance/firstInput.ts","name":null,"original":{"line":73,"column":10},"generated":{"line":65,"column":10}},{"source":"../src/performance/firstInput.ts","name":null,"original":{"line":73,"column":11},"generated":{"line":65,"column":11}},{"source":"../src/performance/firstInput.ts","name":null,"original":{"line":74,"column":0},"generated":{"line":66,"column":0}},{"source":"../src/performance/firstInput.ts","name":null,"original":{"line":74,"column":1},"generated":{"line":66,"column":1}}],"sources":{"../src/performance/firstInput.ts":"import { logData, logMetric } from '../data/log';\nimport { cls, lcp, rt, tbt } from '../data/metrics';\nimport { perfObservers } from './observeInstances';\nimport { poDisconnect } from './performanceObserver';\nimport { PerformanceEventTiming } from '../typings/types';\n\n/**\n * 初始化首次输入延迟监控\n * \n * 该函数在首次输入事件触发后被调用，用于：\n * 1. 计算并记录 FID (First Input Delay) 指标\n * 2. 记录其他核心 Web Vitals 指标\n * 3. 清理性能观察器，避免内存泄露\n * 4. 延迟记录 TBT 和资源消耗数据\n * \n * @param performanceEntries - 性能事件时间数组，包含输入事件的详细信息\n */\nexport const initFirstInputDelay = (\n  performanceEntries: PerformanceEventTiming[]\n) => {\n  // 取最后一位即为我们希望所获取的时间点\n  // 因为 FID 观察器可能触发多次，我们只需要最后一次的输入事件\n  const lastEntry = performanceEntries.pop();\n  \n  if (lastEntry) {\n    // Core Web Vitals FID 逻辑\n    // 测量输入事件的延迟操作：从事件开始到开始处理的时间差\n    // 这是衡量页面响应性的关键指标\n    logMetric(lastEntry.processingStart - lastEntry.startTime, 'fidVitals', {\n      performanceEntry: lastEntry,\n    });\n    \n    // 传统的 FID 逻辑\n    // 测量处理第一个输入事件的持续时间\n    // 包括事件处理的总时间\n    logMetric(lastEntry.duration, 'fid', {\n      performanceEntry: lastEntry,\n    });\n  }\n  \n  // 销毁对 FID 的注册回调，避免过多的观察者造成内存泄露\n  poDisconnect(1);\n  \n  // 初始化并记录 LCP (Largest Contentful Paint) 指标\n  logMetric(lcp.value, 'lcp');\n  \n  // 如果存在 LCP 观察器，立即获取其记录\n  // takeRecords() 方法可以立即获取所有待处理的性能条目\n  if (perfObservers[3] && typeof perfObservers[3].takeRecords === 'function') {\n    perfObservers[3].takeRecords();\n  }\n  \n  // 记录 CLS (Cumulative Layout Shift) 累积布局偏移指标\n  logMetric(cls.value, 'cls');\n  \n  // 记录 TBT (Total Blocking Time) 总阻塞时间指标\n  logMetric(tbt.value, 'tbt');\n  \n  // FID 触发后 5 秒延迟记录 TBT 指标\n  // 这可以反映用户在首次交互后的页面响应性\n  setTimeout(() => {\n    logMetric(tbt.value, `tbt5S`);\n  }, 5000);\n  \n  // FID 触发后 10 秒延迟记录 TBT 指标和资源消耗数据\n  // 这可以反映用户在首次交互后一段时间内的整体体验\n  setTimeout(() => {\n    logMetric(tbt.value, `tbt10S`);\n    \n    // FID 被激活以后 10S 的整体数据消耗\n    // 记录各种资源类型的加载时间消耗\n    logData('dataConsumption', rt.value);\n  }, 10000);\n};\n"},"lineCount":null}},"error":null,"hash":"1f926053b4ae09ab5a1833e97bd36915","cacheData":{"env":{}}}